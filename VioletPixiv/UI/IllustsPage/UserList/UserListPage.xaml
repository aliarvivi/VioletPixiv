<local:UsersPageTemplate x:Class="VioletPixiv.UserListPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:VioletPixiv"
      xmlns:uc="clr-namespace:VioletPixiv.UIObject.UserControls"
      xmlns:vm="clr-namespace:VioletPixiv.ViewModel">

    <Grid >

        <ScrollViewer x:Name="ListViewScrollViewer"
                      ScrollChanged="OnScrollChanged">
            
                <uc:UserListView ItemsSource="{Binding CollectionListPartial, UpdateSourceTrigger=PropertyChanged}"/>
               
        </ScrollViewer>
    </Grid>
</local:UsersPageTemplate>
